[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = sudheer
	email = vsudheerds@gmail.com
[core]
	editor = notepad++ -multiInst -nosession
